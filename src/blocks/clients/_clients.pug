mixin clients
    .clients
        .container
            +title('Clients').clients__title
            .row
                each val, index in ['','','','']
                    .col-12.col-sm-6.col-lg-3.d-flex.align-items-center.justify-content-center
                        .clients__img
                            img(src=`./img/logo${index}.png`, alt="")