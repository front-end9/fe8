mixin latest
    .latest
        .container
            +title('Latest Works').latest__title
            .row
                each val, index in ['MOBILE APP DESIGN', 'WEB DESIGN', 'WEB DEVELOPMENT', 'PRODUCT DESIGN', 'UX & UI', 'ANIMATIONS']
                    .col-12.col-md-6.col-lg-4
                        .latest__panel
                            img(src=`./img/Work ${index + 1}.png`, alt="")